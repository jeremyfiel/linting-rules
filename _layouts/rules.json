---
layout: none
---
{
"rules": {
"{{ page.slug }}":{
{% for property in page[page.slug] %}
    {% if property[1].first %}
        "{{ property[0] }}": {{ property[1] | jsonify  }}{% unless forloop.last %},{% endunless %}
    {% else %}
    {% if property[0]  contains 'x-' %}{% else %}    
        "{{ property[0] }}": "{{ property[1] }}"{% unless forloop.last %},{% endunless %}
    {% endif %}
{% endfor %} 
}
}
}